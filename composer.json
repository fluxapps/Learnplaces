{
    "name": "studer-raimann.ch/learnplaces",
    "description": "Ground up rewrite of the Lernorte plugin for modern ILIAS versions.",
    "type": "project",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Nicolas SchaÌˆfli",
            "email": "ns@studer-raimann.ch"
        }
    ],
    "support": {
        "email": "info@studer-raimann.ch"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "studer-raimann.ch\\Lernplaces\\": "classes"
        },
        "classmap": ["classes/**/*GUI.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "studer-raimann.ch\\Lernplaces\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "sort-packages": true,
        "discard-changes": true,
        "htaccess-protect": true,
        "secure-http": true,
        "process-timeout": 600
    }
}
