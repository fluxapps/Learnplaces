{
  "channelId": "flux-repository",
  "reactsOn": {
    "fetchFromOnline": {
      "address": "{$name}/repository/fetchFromOnline",
      "operationId": "fetchFromOnline",
      "payload": {
        "entityName": {
          "type": "string"
        },
        "address": {
          "type": "string"
        }
      },
      "replyTo": "{$name}/repository/{$entityName}/fetched"
    },
    "changeBaseUrl": {
      "address": "{$name}/repository/changeBaseUrl",
      "operationId": "changeBaseUrl",
      "payload": {
        "baseUrl": {
          "type": "string"
        }
      },
      "replyTo": "{$name}/repository/changedBaseUrl"
    },
    "fetchFromOffline": {
      "address": "{$name}/repository/fetchFromOffline",
      "operationId": "fetchFromOffline",
      "payload": {
        "entityName": {
          "type": "string"
        },
        "address": {
          "type": "string"
        }
      },
      "replyTo": "{$name}/repository/{$entityName}/fetched"
    }
  }
}