{
  "name": "flux-app-learnplaces",
  "src": "./modules/src/Adapters/Api/FluxAppApi.mjs",
  "reactsOn": {
    "onFluxLayoutComponentInitialized": {
      "address": "flux-layout-component/initialized",
      "operationId": "command",
      "payload": {
        "address": "flux-layout-component/createElement",
        "commandPayload": {
          "name": "flux-layout-header"
        }
      }
    },
    "onLayoutHeaderCreated": {
      "address": "flux-layout-component/flux-layout-header/elementCreated",
      "operationId": "command",
      "payload": {
        "address": "flux-layout-component/createSlotElement",
        "commandPayload": {
          "parentName": "flux-layout-header",
          "slotName": "flux-layout-header-menu",
          "name": "flux-layout-menu"
        }
      }
    },
    "onLayoutMenuCreated": {
      "address": "flux-layout-component/flux-layout-header-menu/slotElementCreated",
      "operationId": "command",
      "payload": {
        "address": "flux-layout-component/createContent",
        "commandPayload": {
          "parentName": "flux-layout-header-menu",
          "slotName": "flux-layout-menu-title",
          "name": "flux-layout-menu-title",
          "data": {
            "id": "flux-layout-header-menu-title",
            "value": "Courses"
          }
        }
      }
    },
    "createContentContainer": {
      "address": "flux-repository/initialized",
      "operationId": "command",
      "payload": {
        "address": "flux-layout-component/createElement",
        "commandPayload": {
          "name": "flux-layout-content-container"
        }
      }
    },
    "onRepositoryInitialized": {
      "address": "flux-repository/initialized",
      "operationId": "command",
      "payload": {
        "address": "flux-repository/fetchFromOffline",
        "commandPayload": {
          "parentName": "flux-layout-header-menu",
          "slotName": "flux-layout-menu-item",
          "name": "flux-layout-menu-item",
          "address": "courses/projectIdValueList"
        }
      }
    },
    "loadMap": {
      "address": "flux-layout-component/flux-layout-menu-item/onClicked",
      "operationId": "command",
      "payload": {
        "address": "flux-layout-component/createContent",
        "commandPayload": {
          "parentName": "flux-layout-content-container",
          "slotName": "flux-layout-content-container-content",
          "name": "flux-layout-map",
          "data": {
            "id": "flux-layout-map",
            "value": ""
          }
        }
      }
    }
  }
}